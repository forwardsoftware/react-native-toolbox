{
  "name": "@forward-software/react-native-toolbox",
  "version": "4.1.3",
  "description": "A set of scripts to simplify React Native development",
  "author": "ForWarD Software (https://github.com/forwardsoftware)",
  "license": "MPL-2.0",
  "repository": "https://github.com/forwardsoftware/react-native-toolbox",
  "keywords": [
    "react-native",
    "scripts",
    "utils",
    "development"
  ],
  "homepage": "https://github.com/forwardsoftware/react-native-toolbox#readme",
  "bugs": "https://github.com/forwardsoftware/react-native-toolbox/issues",
  "scripts": {
    "cleanup": "rimraf android/ ios/ dist/ .nyc_output/ oclif.manifest.json .env",
    "build": "rimraf dist && tsc -b",
    "lint": "eslint",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "bin": {
    "rn-toolbox": "./bin/run.js"
  },
  "oclif": {
    "bin": "rn-toolbox",
    "dirname": "rn-toolbox",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "ansis": "^4.0.0",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5",
    "@types/mocha": "^10",
    "@types/node": "^24",
    "chai": "^5",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^11",
    "oclif": "^4",
    "rimraf": "^6",
    "ts-node": "^10",
    "typescript": "^5"
  }
}